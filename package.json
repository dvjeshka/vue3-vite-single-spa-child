{
  "name": "@finuslugi/cuf-backoffice",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "cross-env VUE_APP_BACKOFFICE_API_PREFIX=https://jsonplaceholder.typicode.com TEST=runtimeTEST TITLE=runtimeTitle import-meta-env --example .env.runtime && vite preview",
    "lint": "eslint src/**/*.{ts,vue} --fix",
    "prettier": "prettier -w -u . --ignore-path .gitignore",
    "test": "vitest run --coverage",
    "test-ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "prepare": "husky install",
    "pre-commit": "lint-staged && yarn test"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": "eslint --fix",
    "*": "prettier -w -u --ignore-path .gitignore"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "@vueuse/integrations": "^9.13.0",
    "axios": "^1.6.5",
    "quasar": "^2.14.2",
    "single-spa-vue": "^2.5.1",
    "vue": "^3.4.14",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@import-meta-env/cli": "^0.4.1",
    "@import-meta-env/unplugin": "^0.4.10",
    "@quasar/vite-plugin": "^1.6.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitest/coverage-c8": "^0.25.8",
    "@vitest/ui": "^0.25.8",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.20.1",
    "happy-dom": "^8.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "2.8.1",
    "sass": "^1.70.0",
    "typescript": "^4.9.5",
    "vite": "^4.5.1",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-compress": "^2.1.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-tips": "^2.1.2",
    "vite-plugin-vue-inspector": "^2.0.4",
    "vitest": "^0.25.8",
    "vitest-sonar-reporter": "^0.3.5",
    "vue-eslint-parser": "^9.4.0",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "dotenv": "^16.0.0"
  }
}
