{
  "name": "@finuslugi/cuf-backoffice",
  "version": "0.0.0",
  "private": true,
                "scripts": {
                  "dev": "vite",
                  "build": "vue-tsc --noEmit && vite build",
                  "preview": "vite preview",
                  "lint": "eslint src/**/*.{ts,vue} --fix",
                  "prettier": "prettier -w -u . --ignore-path .gitignore",
                  "husky": {
                    "hooks": {
                      "pre-commit": "lint-staged && yarn test"
                    }
    },
    "lint-staged": {
      "src/**/*.{ts,vue}": "eslint --fix",
      "*": "prettier -w -u --ignore-path .gitignore"
    },
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@quasar/extras": "^1.14.1",
    "prettier": "2.7.1",
    "quasar": "^2.7.3",
    "single-spa-vue": "^2.5.1",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "c8": "^7.11.3",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.1.1",
    "happy-dom": "^5.3.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "sass": "1.32.12",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vite-plugin-checker": "^0.4.6",
    "vite-plugin-compress": "^2.1.1",
    "vite-plugin-eslint": "^1.6.1",
    "vite-plugin-tips": "^2.1.0",
    "vite-plugin-vue-inspector": "^1.0.1",
    "vitest": "^0.16.0",
    "vitest-sonar-reporter": "^0.2.1",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.38.2"
  }
}
