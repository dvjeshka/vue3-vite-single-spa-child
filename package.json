{
  "name": "@finuslugi/cuf-backoffice",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "cross-env VUE_APP_BACKOFFICE_API_PREFIX=https://jsonplaceholder.typicode.com TEST=runtimeTEST TITLE=runtimeTitle import-meta-env --example .env.runtime && vite preview",
    "lint": "eslint src/**/*.{ts,vue} --fix",
    "prettier": "prettier -w -u . --ignore-path .gitignore",
    "test": "vitest run --coverage",
    "test-ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "prepare": "husky install",
    "pre-commit": "lint-staged && yarn test"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": "eslint --fix",
    "*": "prettier -w -u --ignore-path .gitignore"
  },
  "dependencies": {
    "@vueuse/integrations": "^9.6.0",
    "axios": "^1.2.1",
    "sass": "^1.56.2",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@import-meta-env/cli": "^0.4.1",
    "@import-meta-env/unplugin": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.25.8",
    "@vitest/ui": "^0.25.8",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.8.0",
    "happy-dom": "^8.1.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "prettier": "2.8.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vite-plugin-checker": "^0.5.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-tips": "^2.1.0",
    "vite-plugin-vue-inspector": "^2.0.1",
    "vitest": "^0.25.8",
    "vitest-sonar-reporter": "^0.3.3",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.13"
  },
  "peerDependencies": {
    "dotenv": "^16.0.0"
  }
}
